self.queryTimeout = window.setTimeout(function() {
	var key = self.get(&quot;value&quot;);
	if (!key) {
		dg.filter();
	} else {
		dg.filter([{
			junction: &quot;or&quot;,
			criterions: [{
				property: &quot;siteCode&quot;,
				operator: &quot;like&quot;,
				value: key
			}, {
				property: &quot;physicsSiteCode&quot;,
				operator: &quot;like&quot;,
				value: key
			}, {
				property: &quot;siteName&quot;,
				operator: &quot;like&quot;,
				value: key
			}]
		}]);
	}
